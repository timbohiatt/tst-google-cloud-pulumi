{
    "name": "google-cloud-pulumi-examples",
    "version": "0.0.1",
    "private": false,
    "repository": "git@github.com:google-cloud-pulumi/google-cloud-pulumi-examples",
    "author": "Tim Hiatt <timhiatt@google.com>",
    "license": "Apache-2.0",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.40.1",
        "@typescript-eslint/parser": "^5.40.1",
        "eslint": "^8.26.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard-with-typescript": "^23.0.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "prettier": "^2.7.1"
    },
    "lint-staged": {
        "**/pulumi-classic/go/*/*.go": [
            "gofmt -s -w ."
        ],
        "**/pulumi-native/go/*/*.go": [
            "gofmt -s -w ."
        ],
        "*.{js,ts}": [
            "eslint --cache --fix"
        ],
        "*.{js,ts,md,css}": [
            "prettier --write"
        ]
    },
    "scripts": {
        "prepare": "husky install"
    }
}